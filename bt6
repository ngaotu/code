#include <iostream>
#include <cstring>
using namespace std;
int main (){
string str1;
cout<<"Nhap xau: \n";
getline(cin,str1);
cout<<"Xau ki tu:"<<str1<<endl;
cout<<str1.length();
for(int i=0;i<str1.length();i++){
    if(str1.at(i)>='0'&& str1.at(i)<='9'){
       cout<<i<<" ";
    }

}

    return 0;
}
