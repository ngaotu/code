#include <iostream>
#include <cmath>
using namespace std;

int ucln(int a,int b){
    int r,x,y;
    x=a;
    y=b;
    r=x%y;
    while(r!=0){
        x=y;
        y=r;
        r=x%y;
    }
    return y;
}
int bcnn(int a,int b){
    return a*b/ucln(a,b);
}

int main (){
int a,b;
cout<<"Nhap hai so nguyen a va b: ";
cin>>a>>b;
cout<<"\nbcnn: "<<bcnn(a,b);


    return 0;
}
